<div class= "container">
  <div class= "row">
    <div class= "col">
      <h2>犬種</h2>
    </div>
  </div>
  <div class = "row">
    <div class = "col">
      <%= form_with model: @dog_breed, url: admin_dog_breed_path, method: :patch, local: true do |f| %>
      <%= f.label :name, "犬種名" %>
      <%= f.text_field :name %>
      <%= f.submit "編集する", class:"btn btn-success" %>
      <% end %>
    </div>
  </div>
</div>